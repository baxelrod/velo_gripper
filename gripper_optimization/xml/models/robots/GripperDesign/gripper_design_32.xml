<?xml version="1.0" ?>
<robot type="Gripper32">
    <palm>4cm/finger_link_pad_cad.xml</palm>
    <dof type="r">
        <defaultVelocity>1.0</defaultVelocity>
        <maxEffort>2.5e+9</maxEffort>
        <Kp>1.0e+11</Kp>
        <Kd>1.0e+7</Kd>
        <draggerScale>7</draggerScale>
    </dof>
    <dof type="r">
        <defaultVelocity>1.0</defaultVelocity>
        <maxEffort>5.0e+9</maxEffort>
        <Kp>1.0e+11</Kp>
        <Kd>1.0e+7</Kd>
        <draggerScale>7</draggerScale>
    </dof>
    <chain>
        <transform>
            <translation>-40 0 0</translation>
            <rotation>180 y</rotation>
        </transform>
        <joint type="Revolute">
            <theta>d0+10</theta>
            <d>0</d>
            <a>40</a>
            <alpha>0</alpha>
            <minValue>-5</minValue>
            <maxValue>100</maxValue>
            <viscousFriction>5.0e+7</viscousFriction>
            <CoulombFriction>0</CoulombFriction>
            <springStiffness>0</springStiffness>
        </joint>
        <joint type="Revolute">
            <theta>d1+80</theta>
            <d>0</d>
            <a>40</a>
            <alpha>0</alpha>
            <minValue>-100</minValue>
            <maxValue>30</maxValue>
            <viscousFriction>5.0e+7</viscousFriction>
            <CoulombFriction>0</CoulombFriction>
            <springStiffness>0</springStiffness>
        </joint>
        <link dynamicJointType="Revolute">4cm/finger_link_pad_cad.xml</link>
        <link dynamicJointType="Revolute">4cm/finger_link_pad_last_cad.xml</link>
    </chain>

    <tendon name="Main flexor">
        <insertionPoint>
			<chain>-1</chain>
			<link>-1</link>
			<position>-25 -30 0</position>
        </insertionPoint>
        <insertionPoint>
			<chain>-1</chain>
			<link>-1</link>
			<position>-25 5 0</position>
        </insertionPoint>
        <!-- --------------- l0 --------------- -->
        <insertionPoint>
			<chain>-1</chain>
			<link>-1</link>
			<position>-35 3 0</position>
        </insertionPoint>
        <insertionPoint>
			<chain>0</chain>
			<link>0</link>
			<position>-35 3 0</position>
        </insertionPoint>
        <!-- --------------- l1 --------------- -->
        <insertionPoint>
			<chain>0</chain>
			<link>0</link>
			<position>-2 1 0</position>
        </insertionPoint>
        <insertionPoint>
			<chain>0</chain>
			<link>1</link>
			<position>-37 1 0</position>
        </insertionPoint>
    </tendon>

    <tendon name="Main extensor">
        <stiffness>2.0</stiffness>
        <!-- --------------- l0 --------------- -->
        <insertionPoint>
			<chain>-1</chain>
			<link>-1</link>
			<position>-35 -3 0</position>
        </insertionPoint>
        <insertionPoint>
			<chain>0</chain>
			<link>0</link>
			<position>-35 -3 0</position>
        </insertionPoint>
        <!-- --------------- l1 --------------- -->
        <insertionPoint>
			<chain>0</chain>
			<link>0</link>
			<position>0.5 -6 0</position>
        </insertionPoint>
        <insertionPoint>
			<chain>0</chain>
			<link>1</link>
			<position>-37.5 -4 0</position>
        </insertionPoint>
    </tendon>

    <tendonWrapper>
      <chain>-1</chain>
      <link>-1</link>
      <position>-40 0 0</position>
      <orientation>0 0 1</orientation>
      <radius>5</radius>
      <exemption tendon="Main flexor"/>
    </tendonWrapper>

    <tendonWrapper>
      <chain>0</chain>
      <link>0</link>
      <position>0 -4 0</position>
      <orientation>0 0 1</orientation>
      <radius>2</radius>	
      <exemption tendon="Main flexor"/>
    </tendonWrapper>

    <approachDirection>
        <referenceLocation>-15 5 0</referenceLocation>
        <direction>0 1 0</direction>
    </approachDirection>

    <virtualContacts>virtual/gripper_design_13.vgr</virtualContacts>
    <eigenGrasps>eigen/fingertip.egr</eigenGrasps>

</robot>
